
<div class="container">

<div class="row">
    <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
    	<form {{action 'CrearUsuario' firstname lastname username email password on='submit'}}role="form">
			<h2>Registro <small>Nuevo usuario</small></h2>
			<hr class="colorgraph">
			<div class="row">
				<div class="col-xs-12 col-sm-6 col-md-6">
					<div class="form-group">
					{{input value=firstname class="form-control input-lg" name="first_name" id="first_name" placeholder="Nombre" tabindex="1"}}
					</div>
				</div>
				<div class="col-xs-12 col-sm-6 col-md-6">
					<div class="form-group">
					{{input value=lastname class="form-control input-lg" name="last_name" id="last__name" placeholder="Apellido" tabindex="2"}}
					</div>
				</div>
			</div>
			<div class="form-group">
					{{input value=username class="form-control input-lg" name="user_name" id="user_name" placeholder="Nombre de Usuario" tabindex="3"}}
			</div>
			<div class="form-group">
					{{input value=email class="form-control input-lg" name="email" id="email" placeholder="Email" tabindex="4"}}
			</div>
				<div class="form-group">
					<div class="form-group">
						{{input value=password type="password" class="form-control input-lg" name="password" id="password" placeholder="Password" tabindex="5"}}
					</div>
			</div>
	
			<hr class="colorgraph">
			<div class="row">
				<div class="text-center"><input type="submit" value="Registrar" class="btn btn-primary btn-block btn-lg" tabindex="6"></div>
			</div>
		</form>
	</div>
</div>
</div>
